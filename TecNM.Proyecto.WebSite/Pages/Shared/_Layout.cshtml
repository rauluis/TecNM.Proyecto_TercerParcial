                        <a class="dropdown-item" asp-area="" asp-page="/User/List">User</a>
                    </div>
                </li>
                <li class="nav-item">
                    <a asp-area="" asp-page="/About" class="nav-link">About us</a>
                </li>
                <li class="nav-item">
                    <a asp-area="" asp-page="/Contact" class="nav-link">Contact</a>
                </li>
                <li class="nav-item cta cta-colored">
                    <a asp-area="" asp-page="/Cart" class="nav-link"><span class="icon-shopping_cart"></span>[0]</a>
                </li>

            </ul>
